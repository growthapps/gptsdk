{
    "name": "growthapps/gptsdk",
    "description": "Develop ai features in your application 10x faster",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "gpt",
        "llm",
        "prompt management",
        "prompt parametrization",
        "ai logs",
        "prompt history",
        "prompt collaborations"
    ],
    "authors": [
        {
            "name": "AndriiMz",
            "email": "moroz97andrze@gmail.com",
            "homepage": "https://gpt-sdk.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/collections": "^2.1",
        "symfony/http-client": "^6.3"
    },
    "require-dev": {
        "ramsey/devtools": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Growthapps\\Gptsdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Growthapps\\Test\\Gptsdk\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "ramsey/composer-repl": true,
            "ramsey/devtools": true
        },
        "sort-packages": true
    },
    "extra": {
        "captainhook": {
            "force-install": true
        },
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        },
        "ramsey/devtools": {
            "command-prefix": "dev",
            "memory-limit": "-1"
        }
    }
}
